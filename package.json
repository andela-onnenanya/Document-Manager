{
  "name": "okdocs",
  "version": "1.0.0",
  "description": "A simple document management application",
  "main": "server.js",
  "scripts": {
    "start": "./node_modules/.bin/babel server.js",
    "start:dev": "nodemon server.js --exec babel-node",
    "build": "./node_modules/.bin/webpack",
    "build:dev": "./node_modules/.bin/webpack -w",
    "deploy": "npm start",
    "mocha": "NODE_ENV=test mocha server/test/**/*.spec.js --compilers js:babel-register",
    "cover": "rm -rf coverage/ && NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec server/test/*.spec.js",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "db:create:test": "psql -c 'create database okdocs_test;' -U postgres",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:test": "NODE_ENV=test sequelize db:migrate",
    "postinstall": "./node_modules/.bin/webpack",
    "seed:test": "NODE_ENV=test sequelize db:seed:all",
    "test": "npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-onnenanya/Document-Manager.git"
  },
  "keywords": [
    "document",
    "document-manager",
    "document-management"
  ],
  "author": "Nnenanya Obinna K",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-onnenanya/Document-Manager/issues"
  },
  "homepage": "https://github.com/andela-onnenanya/Document-Manager#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.0.0",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "css-loader": "^0.28.2",
    "dotenv": "^4.0.0",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "file-loader": "^0.11.1",
    "history": "^4.6.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-coverflow": "^0.2.3",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.5",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
